class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int slen = nums.size();
        for(int i = 0; i < slen;){
            if(nums[i] == 0){
                nums.erase(nums.begin()+i);
                nums.emplace_back(0);
                --slen;
            } else {
                i++;
            }
        }
    }
};
